{
    "name": "tasmaniski/laminas-config-helper",
    "description": "Provide Controller plugin and View helper for reading a config.",
    "type": "library",
    "keywords": [
        "zend framework",
        "config",
        "helper",
        "plugin"
    ],
    "license": "MIT",
    "homepage": "https://github.com/tasmaniski/laminas-config-helper",
    "authors": [
        {
            "name": "Aleksandar Varnicic",
            "email": "tasmaniski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-config": "^2.6 | ^3.0",
        "laminas/laminas-dependency-plugin": "^1.0 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-0": {
            "ConfigHelper": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "ConfigHelperTest": "test/"
        }
    }
}
